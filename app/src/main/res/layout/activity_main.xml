<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:xhunmon="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context="com.xhunmon.test.MainActivity">

    <com.xhunmon.test.CarBoardView
        android:id="@+id/cbv"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true"
        xhunmon:innerRingColor="#53dee7"
        xhunmon:innerRingRadius="40"
        xhunmon:outRingColor="#96a0cf"
        xhunmon:outRingRadius="120"/>

</RelativeLayout>
